<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="observer.zagart.by.client">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name="observer.zagart.by.client.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/ObserverDefault">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginActivity"/>
        <activity android:name=".activities.GuestActivity"/>
        <activity android:name=".activities.ResultActivity"/>
        <activity android:name=".activities.MyAccountActivity"/>
        <activity android:name=".activities.RegistrationActivity"/>
    </application>
</manifest>
